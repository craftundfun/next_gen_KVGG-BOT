{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"dom",
			"es6"
		],
		"allowJs": true,
		"jsx": "react",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"paths": {
			"@": ["./src"],
			"@modules/*": ["./src/modules/*"],
			"@components/*": ["./src/assets/Components/*"],
			"@customTypes/*": ["./src/customTypes/*"],
			"@context/*": ["./src/context/*"],
			"@lib/*": ["./src/lib/*"],
			"@utils/*": ["./src/assets/lib/*"],
			"@ui/*": ["./src/assets/Components/ui/*"],
		}
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}
